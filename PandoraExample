local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Awakenchan/UI-Librarys-Exmaple/refs/heads/main/PandoraSource"))()
local window =Library:Window({allow_resize = true, size = Vector2.new(610,735)})
local tab = window:Page({Name = "Testing"})
local section = tab:Section({Name  = "section", size = "auto"})
section:Slider({Name = "test", min = 0, max = 100, flag = "test"})
section:List({Name = "test",Options= {"Nigga 1","Nigga 2","Femboy"},Flag= "TestList",CallBack = function(selected) warn(selected) end})
section:Button({Name = 'Test', CallBack = function() warn("test") end})
section:Colorpicker({Name = "Accent", flag = "menu accent", callback = function(v)
    Library:ChangeAccent(v)
end})
section:Toggle({Name = "Togle", State = false, Flag = "exampletoggle", Toggled = false, callback = function(v)
   warn(v)
end}):Keybind({State= false,Mode = "Toggle",Flag = "Eample keybind",Binding = nil, callback = function(v)
	warn(v)
end})
